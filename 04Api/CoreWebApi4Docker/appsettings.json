{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "tokenParameter": {
    "secret": "1234567891234567",//jwt加密密钥
    "issuer": "HXP",             //签发人
    "accessExpiration": 120,     //token有效分钟数
    "refreshExpiration": 1440    //refreshToken的有效分钟数，未过期前可以用它去重新申请token，过了需要重新登录
  }
}
